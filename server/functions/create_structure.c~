t_env	*create_env()
{
  t_env	*env;

  env = NULL;
  env = malloc(sizeof(*env));
  if (env == NULL)
    {
      log(ERROR, "L'environnement n'a pu être créé\n");
    }
  env->channel = NULL;
  log(INFO, "L'environnement a bien été créé");
  return (env);
}
